<?xml version='1.0'?>
<mavlink>
	<include>common.xml</include>
	<!-- Use only message IDs from 150 to 240. All others are reserved for common.xml -->
	<!-- eNose messages and enums, by Andres Gongora - MAPIR - University of Malaga -->


	<!-- ENUMS - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -->
	<enums>
	
		<enum name="GAS_SENSOR_TECHNOLOGY">
			<entry name="GAS_SENSOR_TECHNOLOGY_UNKNOWN" value="0"> <description> pre-initialization</description></entry>
			<entry name="GAS_SENSOR_TECHNOLOGY_MOX" value="1"> <description> pre-initialization</description></entry>
			<entry name="GAS_SENSOR_TECHNOLOGY_ELECTROLYTIC" value="2"> <description> disabled</description></entry>
			<entry name="GAS_SENSOR_TECHNOLOGY_NOT_VALID" value="255"> <description> pre-initialization</description></entry>
		</enum>

		<enum name="GAS_SENSOR_MANUFACTURER">
			<entry name="GAS_SENSOR_MANUFACTURER_UNKNOWN" value="0"> <description> pre-initialization</description></entry>
			<entry name="GAS_SENSOR_MANUFACTURER_FIGARO" value="1"> <description> pre-initialization</description></entry>
			<entry name="GAS_SENSOR_MANUFACTURER_ALPHA_SENSE" value="2"> <description> disabled</description></entry>
			<entry name="GAS_SENSOR_MANUFACTURER_NOT_VALID" value="255"> <description> pre-initialization</description></entry>
		</enum>

	</enums>




	<!-- MESSAGES  - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -->
	<messages>

		<message id="150" name="GAS_SENSOR_MEASUREMENT">
			<description></description>
			<field type="uint8_t" name="technology">Sensor technology, such as MOX or Electrolytic. See enum GAS_SENSOR_TECHNOLOGY</field>
			<field type="uint8_t" name="manufacturer">Manufacturer of the sensor. See enum GAS_SENSOR_MANUFACTURER</field>
			<field type="uint8_t" name="mpn">Manufacturer part number</field>
			<field type="float" name="raw_value">Raw measured value</field>
			<field type="float" name="raw_value_max">Max raw measured value</field>
			<field type="float" name="raw_value_min">Min raw measured value</field>
			<field type="float" name="raw_value_zero">Zero raw measured value</field>
		</message>

	</messages>

</mavlink>
